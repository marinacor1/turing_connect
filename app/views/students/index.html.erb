<div="container">
<div class="col md- 4 card">
  <div class="users-index-page">
    <table class="table">
      <thead class="thead-inverse">
        <tr>
          <th>#</th>
          <th>Cohort</th>
          <th>Name</th>
          <th>Employer</th>
          <th>City</th>
          <th>State</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <% @users.each_with_index do |user, index| %>
            <td><%= index + 1 %></td>
            <td><%= user.cohort %></td>
            <td><%= link_to user.name, user_path(user) %></td>
            <td><%= user.current_employer %></td>
